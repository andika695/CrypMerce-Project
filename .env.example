# Database Configuration
DB_HOST=crypmerce_db
DB_PORT=3306
DB_NAME=crypmerce_database
DB_USER=root
DB_PASS=your_password_here

# Cloudinary Configuration (Get from https://cloudinary.com/console)
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here

# Midtrans Configuration (Get from https://dashboard.midtrans.com/)
MIDTRANS_SERVER_KEY=your_server_key_here
MIDTRANS_CLIENT_KEY=your_client_key_here
MIDTRANS_IS_PRODUCTION=false

# Geoapify for Location & Geocoding (Get from https://myprojects.geoapify.com/)
GEOAPIFY_API_KEY=your_api_key_here